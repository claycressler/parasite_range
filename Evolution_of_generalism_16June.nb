  FormBox[
   RowBox[{
    SubscriptBox["S", "1"], "=", 
    FractionBox[
     RowBox[{"\[Gamma]", " ", 
      SubscriptBox["\[Mu]", "1"]}], 
     RowBox[{"\[Beta]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Lambda]", "1"], "-", 
        SubscriptBox["\[Mu]", "1"]}], ")"}]}]]}], TraditionalForm]]],
 ". "
}], "Text",
